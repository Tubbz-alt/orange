def fib(var n)
	return n if n < 2
	return fib(n - 1) + fib(n - 2)
end

def fib2(var n)
	return fib2(n - 1) + fib2(n - 2) if n >= 2
	return n
end	

def fib3(var n)
	return 0 if n == 0
	return 1 if n == 1
	return fib3(n - 1) + fib3(n - 2)
end

return 1 if fib(8) != 21 or fib2(8) != 21 or fib3(8) != 21 
return 0 