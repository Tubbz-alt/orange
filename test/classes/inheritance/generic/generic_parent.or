class ParentClass
	var a

	def ParentClass(var num)
		a = num
	end
end

class ChildClass from ParentClass
	int b

	def test()
		return a + b
	end

	def ChildClass(var a, int b)
		super(a)

		@b = b
	end
end

var obj = ChildClass(5, 10)
return 1 if obj.test() != 15

var obj2 = ChildClass(5.35, 10)
return 1 if obj2.test() < 15.35
