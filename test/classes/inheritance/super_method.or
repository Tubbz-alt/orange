class ParentClass
	int a
	int b

	def math()
		return a + b
	end

	def ParentClass(int a, int b)
		@a = a
		@b = b
	end
end

class ChildClass from ParentClass
	def math()
		return super.math() * 2
	end

	def ChildClass(int a, int b)
		super(a, b)
	end
end

var p = ParentClass(5, 6)
return 1 if p.math() != 11

var c = ChildClass(5, 6)
return 1 if c.math() != 22 
