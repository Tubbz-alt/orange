# generic members have their 
# types resolved by the constructor called.
class TestClass 
	var a, b 

	def add()
		return a + b
	end 

	def TestClass(var a, var b)
		@a = a 
		@b = b 
	end 
end 

var obj1 = TestClass(3, 5)
return 1 if obj1.add() != 8 

var obj2 = TestClass(3.3, 5)
return 1 if obj2.add() < 8.3

var obj3 = TestClass(3, 5.3)
return 1 if obj3.add() < 8.3 

var obj4 = TestClass(3.3, 5.3)
return 1 if obj4.add() < 8.6